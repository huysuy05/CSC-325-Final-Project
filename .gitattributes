# Enable Git LFS for data files
*.csv filter=lfs diff=lfs merge=lfs -text
*.json filter=lfs diff=lfs merge=lfs
*.parquet filter=lfs diff=lfs merge=lfs
*.xml filter=lfs diff=lfs merge=lfs 
# Enable Git LFS for archives
*.zip filter=lfs diff=lfs merge=lfs
*.tar filter=lfs diff=lfs merge=lfs
*.gz filter=lfs diff=lfs merge=lfs
*.rar filter=lfs diff=lfs merge=lfs
*.7z filter=lfs diff=lfs merge=lfs
*.bin filter=lfs diff=lfs merge=lfs -text
